[Unit]
Description=File Receiver Upload Server
After=network.target

[Service]
Type=simple
User=www-data
Group=www-data
WorkingDirectory=/var/www/quickx
Environment="PATH=/usr/bin:/usr/local/bin"
ExecStart=/usr/bin/python3 /var/www/quickx/upload_server.py
Restart=always
RestartSec=3

[Install]
WantedBy=multi-user.target
